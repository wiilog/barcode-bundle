    "Symfony4",
    "Symfony5",
    "Flex",
    "barcode",
    "Symfony",
    "Bundle",
    "Twig",
    "generator",
    "qrcode",
    "qr code"
  ],
  "license": "Apache-2.0",
  "require": {
